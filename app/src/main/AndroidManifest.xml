<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.matheus.rolity">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar"
        android:windowSoftInputMode="adjustResize">
        <activity android:name="com.matheus.rolity.ActivityUsuario"></activity>
        <activity android:name="com.matheus.rolity.ActivityProducto" />
        <activity android:name="com.matheus.rolity.ActivityMain">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.matheus.rolity.ActivityRegistro" />
        <activity android:name="com.matheus.rolity.ActivityLoginRegister" />
    </application>

</manifest>