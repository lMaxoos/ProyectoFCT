<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.proyecto1">

    <application
        android:name="com.example.rolity.App"
        android:label="@string/app_name"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar"
        android:windowSoftInputMode="adjustResize">

        <activity android:name="com.example.rolity.ActivityMain">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.rolity.ActivityRegistro" />
        <activity android:name="com.example.rolity.ActivityLista" />
        <activity android:name="com.example.rolity.ActivityUsuario" />
        <activity android:name="com.example.rolity.ActivityLogin" />
        <activity android:name="com.example.rolity.ActivityPatin" />

    </application>

</manifest>